// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model FormData {
  id    Int     @id @default(autoincrement())
  serialNo  Int 
  supplierName  String
  farmerName  String
  typeItem String 
  nug Int
  customerName String
  itemName String
  date DateTime
  vclNo String
  freightRate Float
  otherCharge Float
  labourRate  Float 
  // weight String?
}
