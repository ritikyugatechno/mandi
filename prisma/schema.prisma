// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model FormData {
  id    Int     @id @default(autoincrement())
  serialNo  Int?
  supplierName  String?
  farmerName  String?
  lot String?
  typeItem String?
  cut Int?
  cNug Int?
  sNug Int?
  customerName String?
  supplierRate Int?
  customerRate Int?
  itemName String?
  date DateTime?
  vclNo String?
  freightRate Float?
  otherCharge Float?
  labourRate  Float?
}
